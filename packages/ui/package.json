{
    "name": "@invoice/ui",
    "version": "0.0.1",
    "author": "Sandip Gyawali",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "description": "User Interface module for frontend",
    "license": "ISC",
    "scripts": {
        "lint": "turbo lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "type-check": "tsc --noEmit"
    },
    "packageManager": "bun@1.1.42",
    "private": true,
    "engines": {
        "node": ">=23"
    },
    "exports": {
        "./*": "./src/*.tsx",
        "./hooks/*": "./src/hooks/*.ts",
        "./lib/*": "./src/lib/*.ts"
    },
    "keywords": [
        "ui",
        "user-interface",
        "typescript",
        "components"
    ],
    "dependencies": {
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-popover": "^1.1.14",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tooltip": "^1.2.7",
        "sonner": "^2.0.5",
        "recharts": "^2.15.3",
        "react": "^19.0.0",
        "react-day-picker": "^9.7.0",
        "react-dom": "^19.0.0",
        "@invoice/tailwind": "*"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "typescript": "^5"
    }
}